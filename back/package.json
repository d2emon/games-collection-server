{
  "name": "game-collection-server",
  "version": "0.0.2",
  "description": "Game collection server",
  "private": true,
  "main": "./src/index.js",
  "bin": {
    "app": "./src/index.js"
  },
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./dist",
    "debug": "node --debug-brk src/app.js",
    "dev": "supervisor src/app.js",
    "serve": "DEBUG=games-collection nodemon --watch src -e ts,ejs --exec npm run start",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d2emon/games-collection-server.git"
  },
  "author": "Dmitry Kutsenko",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/d2emon/games-collection-server/issues"
  },
  "homepage": "https://github.com/d2emon/games-collection-server#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.11",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.10",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "game",
    "collection"
  ]
}
