{
  "name": "game-collection-server",
  "version": "0.0.3",
  "description": "Game collection server",
  "private": true,
  "main": "./src/index.js",
  "bin": {
    "app": "./src/index.js"
  },
  "scripts": {
    "lint": "eslint --fix ./src --ext .ts",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./dist",
    "debug": "node --debug-brk src/app.ts",
    "dev": "supervisor src/app.ts",
    "serve": "DEBUG=games-collection nodemon --watch src -e ts,ejs --exec npm run start",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d2emon/games-collection-server.git"
  },
  "author": "Dmitry Kutsenko",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/d2emon/games-collection-server/issues"
  },
  "homepage": "https://github.com/d2emon/games-collection-server#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "chai": "^4.2.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "game",
    "collection"
  ]
}
